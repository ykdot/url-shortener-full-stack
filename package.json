{
  "name": "url-shortener-full-stack-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:client": "npm run dev --workspace=url-shortener-full-stack",
    "dev:server": "npm run dev --workspace=url-shortener-server",
    "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
    "build": "npm run build --workspace=url-shortener-full-stack"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2"
  }
}
